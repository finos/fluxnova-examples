<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1"
    targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.39.0">
    <bpmn:process id="PostTradeSettlementProcess" name="Post-Trade Settlement Process"
        isExecutable="true">
        <bpmn:startEvent id="StartEvent_TradeReadyForSettlement" name="Trade Ready for Settlement"
            camunda:formKey="embedded:app:forms/trade-ready-form.html">
            <bpmn:extensionElements>
                <camunda:executionListener delegateExpression="${tradeReadyListener}" event="start" />
            </bpmn:extensionElements>
            <bpmn:outgoing>SequenceFlow_1</bpmn:outgoing>
        </bpmn:startEvent>
        <bpmn:userTask id="UserTask_EnrichSettlementInstructions"
            name="Enrich Settlement Instructions" camunda:assignee="${middleOfficeUser}"
            camunda:candidateGroups="middle-office">
            <bpmn:extensionElements>
                <camunda:formData>
                    <camunda:formField id="ssiRetrieved" label="SSI Retrieved" type="boolean" />
                    <camunda:formField id="tradeData" label="Trade Specific Data" type="string" />
                    <camunda:formField id="instructionsComplete" label="Instructions Complete"
                        type="boolean" />
                    <camunda:formField id="settlementDate" label="Settlement Date" type="date" />
                </camunda:formData>
                <camunda:inputOutput>
                    <camunda:inputParameter name="tradeId">${tradeId}</camunda:inputParameter>
                    <camunda:outputParameter name="instructionsComplete">${instructionsComplete}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_1</bpmn:incoming>
            <bpmn:incoming>SequenceFlow_FromRemediation</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_2</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:exclusiveGateway id="ExclusiveGateway_DataValid" name="Data Complete?">
            <bpmn:incoming>SequenceFlow_2</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_DataIncomplete</bpmn:outgoing>
            <bpmn:outgoing>SequenceFlow_DataComplete</bpmn:outgoing>
        </bpmn:exclusiveGateway>
        <bpmn:userTask id="UserTask_DataRemediation" name="Data Remediation"
            camunda:assignee="${operationsUser}" camunda:candidateGroups="operations">
            <bpmn:extensionElements>
                <camunda:formData>
                    <camunda:formField id="missingDataRequested"
                        label="Missing Data Requested from Front Office" type="boolean" />
                    <camunda:formField id="remediationNotes" label="Remediation Notes" type="string" />
                </camunda:formData>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_DataIncomplete</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_FromRemediation</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:serviceTask id="ServiceTask_GenerateSettlementMessage"
            name="Generate Settlement Instruction Message"
            camunda:delegateExpression="${settlementMessageGenerator}">
            <bpmn:extensionElements>
                <camunda:inputOutput>
                    <camunda:inputParameter name="messageFormat">ISO20022</camunda:inputParameter>
                    <camunda:inputParameter name="attachIdentifiers">true</camunda:inputParameter>
                    <camunda:inputParameter name="applyDigitalSigning">true</camunda:inputParameter>
                    <camunda:outputParameter name="settlementMessage">${settlementMessage}</camunda:outputParameter>
                    <camunda:outputParameter name="messageId">${messageId}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_DataComplete</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_3</bpmn:outgoing>
        </bpmn:serviceTask>
        <bpmn:serviceTask id="ServiceTask_TransmitToCustodian" name="Transmit to Custodian/CCP"
            camunda:delegateExpression="${custodianTransmissionService}">
            <bpmn:extensionElements>
                <camunda:inputOutput>
                    <camunda:inputParameter name="transmissionChannel">SWIFT</camunda:inputParameter>
                    <camunda:inputParameter name="settlementMessage">${settlementMessage}</camunda:inputParameter>
                    <camunda:outputParameter name="transmissionId">${transmissionId}</camunda:outputParameter>
                    <camunda:outputParameter name="acknowledgmentReceived">${acknowledgmentReceived}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_3</bpmn:incoming>
            <bpmn:incoming>SequenceFlow_RetryTransmission</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_4</bpmn:outgoing>
        </bpmn:serviceTask>
        <bpmn:serviceTask id="ServiceTask_MonitorSettlementStatus" name="Monitor Settlement Status"
            camunda:delegateExpression="${settlementMonitoringService}">
            <bpmn:extensionElements>
                <camunda:inputOutput>
                    <camunda:inputParameter name="transmissionId">${transmissionId}</camunda:inputParameter>
                    <camunda:inputParameter name="pollingInterval">PT1H</camunda:inputParameter>
                    <camunda:outputParameter name="settlementStatus">${settlementStatus}</camunda:outputParameter>
                    <camunda:outputParameter name="settlementComplete">${settlementComplete}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_4</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_5</bpmn:outgoing>
        </bpmn:serviceTask>
        <bpmn:boundaryEvent id="BoundaryEvent_SettlementTimeout" name="Settlement Timeout (48h)"
            attachedToRef="ServiceTask_MonitorSettlementStatus">
            <bpmn:outgoing>SequenceFlow_Timeout</bpmn:outgoing>
            <bpmn:timerEventDefinition>
                <bpmn:timeDuration>P2D</bpmn:timeDuration>
            </bpmn:timerEventDefinition>
        </bpmn:boundaryEvent>
        <bpmn:exclusiveGateway id="ExclusiveGateway_SettlementStatus" name="Settled?">
            <bpmn:incoming>SequenceFlow_5</bpmn:incoming>
            <bpmn:incoming>SequenceFlow_Timeout</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_Settled</bpmn:outgoing>
            <bpmn:outgoing>SequenceFlow_Failed</bpmn:outgoing>
        </bpmn:exclusiveGateway>
        <bpmn:serviceTask id="ServiceTask_SettlementCompletion" name="Settlement Completion Process"
            camunda:delegateExpression="${settlementCompletionService}">
            <bpmn:extensionElements>
                <camunda:inputOutput>
                    <camunda:inputParameter name="updatePositions">true</camunda:inputParameter>
                    <camunda:inputParameter name="createReconciliationJobs">true</camunda:inputParameter>
                    <camunda:inputParameter name="notifyTeams">true</camunda:inputParameter>
                    <camunda:outputParameter name="settlementConfirmed">${settlementConfirmed}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_Settled</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_ToSuccessEnd</bpmn:outgoing>
        </bpmn:serviceTask>
        <bpmn:serviceTask id="ServiceTask_CreateSettlementFailCase"
            name="Create Settlement Fail Case"
            camunda:delegateExpression="${settlementFailCaseService}">
            <bpmn:extensionElements>
                <camunda:inputOutput>
                    <camunda:inputParameter name="failureReason">${failureReason}</camunda:inputParameter>
                    <camunda:inputParameter name="transmissionId">${transmissionId}</camunda:inputParameter>
                    <camunda:outputParameter name="failCaseId">${failCaseId}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_Failed</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_6</bpmn:outgoing>
        </bpmn:serviceTask>
        <bpmn:userTask id="UserTask_FailResolution" name="Fail Resolution"
            camunda:assignee="${backOfficeUser}" camunda:candidateGroups="back-office">
            <bpmn:extensionElements>
                <camunda:formData>
                    <camunda:formField id="rootCauseAnalysis" label="Root Cause Analysis"
                        type="string" />
                    <camunda:formField id="correctionMade" label="Correction Made" type="boolean" />
                    <camunda:formField id="retryRecommended" label="Retry Recommended"
                        type="boolean" />
                    <camunda:formField id="retryCount" label="Retry Count" type="long"
                        defaultValue="0" />
                </camunda:formData>
                <camunda:inputOutput>
                    <camunda:inputParameter name="failCaseId">${failCaseId}</camunda:inputParameter>
                    <camunda:outputParameter name="retryCount">${retryCount + 1}</camunda:outputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_6</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_7</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:exclusiveGateway id="ExclusiveGateway_RetryDecision" name="Retry Settlement?">
            <bpmn:incoming>SequenceFlow_7</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_Retry</bpmn:outgoing>
            <bpmn:outgoing>SequenceFlow_Escalate</bpmn:outgoing>
        </bpmn:exclusiveGateway>
        <bpmn:userTask id="UserTask_Escalation" name="Escalation"
            camunda:assignee="${seniorOperationsManager}"
            camunda:candidateGroups="senior-management">
            <bpmn:extensionElements>
                <camunda:formData>
                    <camunda:formField id="escalationReason" label="Escalation Reason" type="string" />
                    <camunda:formField id="incidentCreated" label="Incident Record Created"
                        type="boolean" />
                    <camunda:formField id="managementNotified" label="Management Notified"
                        type="boolean" />
                </camunda:formData>
                <camunda:inputOutput>
                    <camunda:inputParameter name="failCaseId">${failCaseId}</camunda:inputParameter>
                    <camunda:inputParameter name="retryCount">${retryCount}</camunda:inputParameter>
                </camunda:inputOutput>
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_Escalate</bpmn:incoming>
            <bpmn:outgoing>SequenceFlow_ToEscalationEnd</bpmn:outgoing>
        </bpmn:userTask>
        <bpmn:endEvent id="EndEvent_SettlementSuccessful" name="Settlement Successful">
            <bpmn:extensionElements>
                <camunda:executionListener delegateExpression="${settlementSuccessListener}"
                    event="start" />
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_ToSuccessEnd</bpmn:incoming>
        </bpmn:endEvent>
        <bpmn:endEvent id="EndEvent_SettlementEscalated" name="Settlement Escalated/Failed">
            <bpmn:extensionElements>
                <camunda:executionListener delegateExpression="${settlementEscalationListener}"
                    event="start" />
            </bpmn:extensionElements>
            <bpmn:incoming>SequenceFlow_ToEscalationEnd</bpmn:incoming>
        </bpmn:endEvent>
        <bpmn:sequenceFlow id="SequenceFlow_1" sourceRef="StartEvent_TradeReadyForSettlement"
            targetRef="UserTask_EnrichSettlementInstructions" />
        <bpmn:sequenceFlow id="SequenceFlow_2" sourceRef="UserTask_EnrichSettlementInstructions"
            targetRef="ExclusiveGateway_DataValid" />
        <bpmn:sequenceFlow id="SequenceFlow_DataIncomplete" name="No"
            sourceRef="ExclusiveGateway_DataValid" targetRef="UserTask_DataRemediation">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!instructionsComplete}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="SequenceFlow_DataComplete" name="Yes"
            sourceRef="ExclusiveGateway_DataValid" targetRef="ServiceTask_GenerateSettlementMessage">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${instructionsComplete}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="SequenceFlow_FromRemediation" sourceRef="UserTask_DataRemediation"
            targetRef="UserTask_EnrichSettlementInstructions" />
        <bpmn:sequenceFlow id="SequenceFlow_3" sourceRef="ServiceTask_GenerateSettlementMessage"
            targetRef="ServiceTask_TransmitToCustodian" />
        <bpmn:sequenceFlow id="SequenceFlow_4" sourceRef="ServiceTask_TransmitToCustodian"
            targetRef="ServiceTask_MonitorSettlementStatus" />
        <bpmn:sequenceFlow id="SequenceFlow_5" sourceRef="ServiceTask_MonitorSettlementStatus"
            targetRef="ExclusiveGateway_SettlementStatus" />
        <bpmn:sequenceFlow id="SequenceFlow_Timeout" sourceRef="BoundaryEvent_SettlementTimeout"
            targetRef="ExclusiveGateway_SettlementStatus" />
        <bpmn:sequenceFlow id="SequenceFlow_Settled" name="Yes"
            sourceRef="ExclusiveGateway_SettlementStatus"
            targetRef="ServiceTask_SettlementCompletion">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${settlementComplete}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="SequenceFlow_Failed" name="No"
            sourceRef="ExclusiveGateway_SettlementStatus"
            targetRef="ServiceTask_CreateSettlementFailCase">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!settlementComplete}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="SequenceFlow_6" sourceRef="ServiceTask_CreateSettlementFailCase"
            targetRef="UserTask_FailResolution" />
        <bpmn:sequenceFlow id="SequenceFlow_7" sourceRef="UserTask_FailResolution"
            targetRef="ExclusiveGateway_RetryDecision" />
        <bpmn:sequenceFlow id="SequenceFlow_Retry" name="Yes"
            sourceRef="ExclusiveGateway_RetryDecision" targetRef="ServiceTask_TransmitToCustodian">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${retryRecommended and
                retryCount &lt; 3}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="SequenceFlow_RetryTransmission"
            sourceRef="ExclusiveGateway_RetryDecision" targetRef="ServiceTask_TransmitToCustodian" />
        <bpmn:sequenceFlow id="SequenceFlow_Escalate" name="No"
            sourceRef="ExclusiveGateway_RetryDecision" targetRef="UserTask_Escalation">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!retryRecommended or
                retryCount &gt;= 3}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="SequenceFlow_ToSuccessEnd"
            sourceRef="ServiceTask_SettlementCompletion" targetRef="EndEvent_SettlementSuccessful" />
        <bpmn:sequenceFlow id="SequenceFlow_ToEscalationEnd" sourceRef="UserTask_Escalation"
            targetRef="EndEvent_SettlementEscalated" />
    </bpmn:process>
    <bpmndi:BPMNDiagram id="BPMNDiagram_1">
        <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="PostTradeSettlementProcess">
