            </bpmndi:BPMNEdge>
            <bpmndi:BPMNEdge id="SequenceFlow_Escalate_di" bpmnElement="SequenceFlow_Escalate">
                <di:waypoint x="720" y="465" />
                <di:waypoint x="720" y="510" />
                <bpmndi:BPMNLabel>
                    <dc:Bounds x="692" y="464" width="15" height="14" />
