    <process id="Process_ClientOnboardingKYC" name="Client Onboarding &#38; KYC (Pre-Trade)"
        isExecutable="true">
        <subProcess id="EventSubProcess_SLA" name="Process SLA (PT120H)" triggeredByEvent="true">
            <startEvent id="StartEvent_ProcessSLA" name="Process SLA Timer" isInterrupting="false">
                <timerEventDefinition>
                    <timeDuration xsi:type="tFormalExpression">PT120H</timeDuration>
                </timerEventDefinition>
            </startEvent>
            <sequenceFlow id="Flow_SLA_Start_To_Escalate" sourceRef="StartEvent_ProcessSLA"
                targetRef="ServiceTask_SLAEscalation_SeniorMgmt" />
            <serviceTask id="ServiceTask_SLAEscalation_SeniorMgmt"
                name="Escalate to Senior Management" camunda:type="external"
                camunda:topic="sla-escalation" />
            <sequenceFlow id="Flow_SLA_Escalate_To_Audit"
                sourceRef="ServiceTask_SLAEscalation_SeniorMgmt"
                targetRef="ServiceTask_MarkCaseForAudit" />
            <serviceTask id="ServiceTask_MarkCaseForAudit" name="Mark Case for Audit"
                camunda:type="external" camunda:topic="mark-for-audit" />
            <sequenceFlow id="Flow_SLA_Audit_To_End" sourceRef="ServiceTask_MarkCaseForAudit"
                targetRef="EndEvent_SLAHandled" />
            <endEvent id="EndEvent_SLAHandled" name="SLA Handling Complete" />
        </subProcess>
        <startEvent id="StartEvent_ApplicationSubmitted" name="Application Submitted" />
        <sequenceFlow id="Flow_Start_To_Capture" sourceRef="StartEvent_ApplicationSubmitted"
            targetRef="UserTask_CaptureClientData" />
        <userTask id="UserTask_CaptureClientData" name="Capture Client Data" />
        <sequenceFlow id="Flow_Capture_To_Persist" sourceRef="UserTask_CaptureClientData"
            targetRef="ServiceTask_PersistClientData" />
        <serviceTask id="ServiceTask_PersistClientData" name="Persist Client Data"
            camunda:type="external" camunda:topic="crm-sync" />
        <sequenceFlow id="Flow_Persist_To_CollectDocs" sourceRef="ServiceTask_PersistClientData"
            targetRef="UserTask_UploadDocuments" />
        <userTask id="UserTask_UploadDocuments" name="Upload Documents &#38; References" />
        <sequenceFlow id="Flow_CollectDocs_To_StoreDocs" sourceRef="UserTask_UploadDocuments"
            targetRef="ServiceTask_StoreDocuments" />
        <serviceTask id="ServiceTask_StoreDocuments" name="Store Documents &#38; References"
            camunda:type="external" camunda:topic="document-management" />
        <sequenceFlow id="Flow_StoreDocs_To_VerifyDocs" sourceRef="ServiceTask_StoreDocuments"
            targetRef="ServiceTask_VerifyDocuments" />
        <serviceTask id="ServiceTask_VerifyDocuments" name="Verify Documents (OCR/Biometrics/IDV)"
            camunda:type="external" camunda:topic="doc-verification" />
        <boundaryEvent id="BoundaryTimer_VerifyDocs" name="48h SLA" cancelActivity="false"
            attachedToRef="ServiceTask_VerifyDocuments">
            <timerEventDefinition>
                <timeDuration xsi:type="tFormalExpression">PT48H</timeDuration>
            </timerEventDefinition>
        </boundaryEvent>
        <sequenceFlow id="Flow_Boundary_Verify_To_EscalateOps" sourceRef="BoundaryTimer_VerifyDocs"
            targetRef="ServiceTask_EscalateOps_VerificationSLA" />
        <serviceTask id="ServiceTask_EscalateOps_VerificationSLA"
            name="Escalate to Ops Manager (Verification SLA Breach)" camunda:type="external"
            camunda:topic="ops-escalation" />
        <sequenceFlow id="Flow_VerifyDocs_To_GwDocs" sourceRef="ServiceTask_VerifyDocuments"
            targetRef="Gateway_DocumentsVerified" />
        <exclusiveGateway id="Gateway_DocumentsVerified" name="Documents Verified?" />
        <sequenceFlow id="Flow_GwDocs_To_Sanctions" name="Yes" sourceRef="Gateway_DocumentsVerified"
            targetRef="ServiceTask_SanctionsPEP">
            <conditionExpression xsi:type="tFormalExpression">${documentsVerified == true}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="Flow_GwDocs_To_Repair" name="No" sourceRef="Gateway_DocumentsVerified"
            targetRef="UserTask_OpsRemediation">
            <conditionExpression xsi:type="tFormalExpression">${!documentsVerified}</conditionExpression>
        </sequenceFlow>
        <userTask id="UserTask_OpsRemediation"
            name="Ops Remediation (Correct Data / Re-request Docs)">
            <incoming>Flow_1srkuh4</incoming>
            <outgoing>Flow_0y9ulo7</outgoing>
            <outgoing>Flow_1431pu5</outgoing>
        </userTask>
        <serviceTask id="ServiceTask_SanctionsPEP" name="Sanctions &#38; PEP Screening"
            camunda:type="external" camunda:topic="sanctions-screen" />
        <sequenceFlow id="Flow_Sanctions_To_GwSanctions" sourceRef="ServiceTask_SanctionsPEP"
            targetRef="Gateway_SanctionsDecision" />
        <exclusiveGateway id="Gateway_SanctionsDecision" name="Sanctions Match?" />
        <sequenceFlow id="Flow_GwSanctions_To_Investigation" name="Yes"
            sourceRef="Gateway_SanctionsDecision"
            targetRef="UserTask_ComplianceInvestigation_Sanctions">
            <conditionExpression xsi:type="tFormalExpression">${sanctionsMatch == true}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="Flow_GwSanctions_To_Risk" name="No" sourceRef="Gateway_SanctionsDecision"
            targetRef="BusinessRule_RiskAssessment">
            <conditionExpression xsi:type="tFormalExpression">${!sanctionsMatch}</conditionExpression>
        </sequenceFlow>
        <userTask id="UserTask_ComplianceInvestigation_Sanctions"
            name="Compliance Investigation (Sanctions Hit)" />
        <sequenceFlow id="Flow_Investigation_To_GwOutcome"
            sourceRef="UserTask_ComplianceInvestigation_Sanctions"
            targetRef="Gateway_SanctionsOutcome" />
        <exclusiveGateway id="Gateway_SanctionsOutcome" name="Sanctions Cleared?" />
        <sequenceFlow id="Flow_GwOutcome_To_Risk" name="Yes" sourceRef="Gateway_SanctionsOutcome"
            targetRef="BusinessRule_RiskAssessment">
            <conditionExpression xsi:type="tFormalExpression">${sanctionsCleared == true}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="Flow_GwOutcome_To_EndRejected" name="No"
            sourceRef="Gateway_SanctionsOutcome" targetRef="EndEvent_Rejected">
            <conditionExpression xsi:type="tFormalExpression">${!sanctionsCleared}</conditionExpression>
        </sequenceFlow>
        <businessRuleTask id="BusinessRule_RiskAssessment" name="AML/KYC Risk Scoring (DMN)"
            camunda:resultVariable="riskCategory" camunda:decisionRef="RiskAssessment"
            camunda:mapDecisionResult="singleEntry" />
        <boundaryEvent id="BoundaryTimer_RiskAssessment" name="24h SLA" cancelActivity="false"
            attachedToRef="BusinessRule_RiskAssessment">
            <timerEventDefinition>
                <timeDuration xsi:type="tFormalExpression">PT24H</timeDuration>
            </timerEventDefinition>
        </boundaryEvent>
        <sequenceFlow id="Flow_Boundary_Risk_To_EscalateCompliance"
            sourceRef="BoundaryTimer_RiskAssessment"
            targetRef="ServiceTask_EscalateCompliance_RiskSLA" />
        <serviceTask id="ServiceTask_EscalateCompliance_RiskSLA"
            name="Escalate to Compliance Manager (Risk Assessment SLA)" camunda:type="external"
            camunda:topic="compliance-escalation" />
        <sequenceFlow id="Flow_Risk_To_GwRiskCategory" sourceRef="BusinessRule_RiskAssessment"
            targetRef="Gateway_RiskCategory" />
        <exclusiveGateway id="Gateway_RiskCategory" name="Risk Category Decision">
            <outgoing>Flow_GwRisk_To_AccountOpening</outgoing>
        </exclusiveGateway>
        <sequenceFlow id="Flow_GwRisk_To_ComplianceReview" name="Medium"
            sourceRef="Gateway_RiskCategory" targetRef="UserTask_ComplianceReview">
            <conditionExpression xsi:type="tFormalExpression">${riskCategory == 'Medium'}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="Flow_GwRisk_To_EDD" name="High" sourceRef="Gateway_RiskCategory"
            targetRef="UserTask_EnhancedDueDiligence">
            <conditionExpression xsi:type="tFormalExpression">${riskCategory == 'High'}</conditionExpression>
        </sequenceFlow>
        <userTask id="UserTask_EnhancedDueDiligence"
            name="Enhanced Due Diligence (Senior Compliance)" />
        <sequenceFlow id="Flow_EDD_To_ComplianceReview" sourceRef="UserTask_EnhancedDueDiligence"
            targetRef="Gateway_ComplianceDecision" />
        <userTask id="UserTask_ComplianceReview" name="Compliance Review" />
        <sequenceFlow id="Flow_ComplianceReview_To_GwDecision" sourceRef="UserTask_ComplianceReview"
            targetRef="Gateway_ComplianceDecision" />
        <exclusiveGateway id="Gateway_ComplianceDecision" name="Approval Decision">
            <outgoing>Flow_GwDecision_To_AccountOpening</outgoing>
        </exclusiveGateway>
        <sequenceFlow id="Flow_GwDecision_To_EndRejected" name="Rejected"
            sourceRef="Gateway_ComplianceDecision" targetRef="EndEvent_Rejected">
            <conditionExpression xsi:type="tFormalExpression">${approvalDecision == 'rejected'}</conditionExpression>
        </sequenceFlow>
        <serviceTask id="ServiceTask_AccountOpening" name="Account Opening / Enable Trading"
            camunda:type="external" camunda:topic="account-provisioning">
            <incoming>Flow_GwRisk_To_AccountOpening</incoming>
            <incoming>Flow_GwDecision_To_AccountOpening</incoming>
        </serviceTask>
        <sequenceFlow id="Flow_AccountOpening_To_Notify" sourceRef="ServiceTask_AccountOpening"
            targetRef="ServiceTask_NotifySalesClient" />
        <serviceTask id="ServiceTask_NotifySalesClient" name="Notify Sales &#38; Client"
            camunda:type="external" camunda:topic="notifications" />
        <sequenceFlow id="Flow_Notify_To_RecordAudit" sourceRef="ServiceTask_NotifySalesClient"
            targetRef="ServiceTask_RecordAuditTrail" />
        <serviceTask id="ServiceTask_RecordAuditTrail" name="Record Audit Trail"
            camunda:type="external" camunda:topic="audit-log" />
        <sequenceFlow id="Flow_RecordAudit_To_EndComplete" sourceRef="ServiceTask_RecordAuditTrail"
            targetRef="EndEvent_OnboardingComplete" />
        <endEvent id="EndEvent_OnboardingComplete" name="Onboarding Complete" />
        <endEvent id="EndEvent_Rejected" name="Rejected (Reason Logged)">
            <incoming>Flow_0ylfm7q</incoming>
        </endEvent>
        <sequenceFlow id="Flow_1srkuh4" sourceRef="ServiceTask_EscalateOps_VerificationSLA"
            targetRef="UserTask_OpsRemediation" />
        <exclusiveGateway id="Gateway_0xqpgfx" name="Proceed with Re-Verification?">
            <incoming>Flow_0y9ulo7</incoming>
            <incoming>Flow_1431pu5</incoming>
            <outgoing>Flow_06w37vo</outgoing>
            <outgoing>Flow_0ylfm7q</outgoing>
        </exclusiveGateway>
        <sequenceFlow id="Flow_0y9ulo7" sourceRef="UserTask_OpsRemediation"
            targetRef="Gateway_0xqpgfx" />
        <sequenceFlow id="Flow_06w37vo" name="Yes" sourceRef="Gateway_0xqpgfx"
            targetRef="ServiceTask_VerifyDocuments" />
        <sequenceFlow id="Flow_1431pu5" sourceRef="UserTask_OpsRemediation"
            targetRef="Gateway_0xqpgfx" />
        <sequenceFlow id="Flow_0ylfm7q" name="No" sourceRef="Gateway_0xqpgfx"
            targetRef="EndEvent_Rejected" />
        <sequenceFlow id="Flow_GwDecision_To_AccountOpening" name="Approved"
            sourceRef="Gateway_ComplianceDecision" targetRef="ServiceTask_AccountOpening">
            <conditionExpression xsi:type="tFormalExpression">${approvalDecision == 'approved'}</conditionExpression>
        </sequenceFlow>
        <sequenceFlow id="Flow_GwRisk_To_AccountOpening" name="Low" sourceRef="Gateway_RiskCategory"
            targetRef="ServiceTask_AccountOpening">
            <conditionExpression xsi:type="tFormalExpression">${riskCategory == 'Low'}</conditionExpression>
        </sequenceFlow>
    </process>
